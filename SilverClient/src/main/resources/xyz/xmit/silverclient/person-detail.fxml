      <TextField fx:id="tfAddressLine2" editable="false" layoutX="429.0" layoutY="44.0" prefHeight="25.0" prefWidth="357.0" />
      <TextField fx:id="tfCity" editable="false" layoutX="429.0" layoutY="81.0" prefHeight="25.0" prefWidth="149.0" />
